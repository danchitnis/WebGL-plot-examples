{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "typeRoots": ["node_modules/@types", "@types"],
    "outDir": "./dist",
    "baseUrl": "./src"
  },
  "include": [
    "src",
    "types"
,  ]
}
